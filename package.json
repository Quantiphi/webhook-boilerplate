{
  "name": "webhook-boilerplate",
  "version": "v1.0.0",
  "description": "This framework built in Node.js makes it simple to create webhook for dialogflow agent by creating a file for a particular intent in the intents library.",
  "author": "Quantiphi, Inc.",
  "email": "support.qonversationalai@quantiphi.com",
  "license": "Apache-2.0",
  "main": "server.js",
  "scripts": {
    "build": "tsc server.ts --outDir dist",
    "start": "npm run build && node dist/server.js",
    "test": "tsc ./specs/<path_to_test_file>.spec.ts --outDir dist/specs && mocha 'dist/specs/**/*.spec.js'"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Quantiphi/webhook-boilerplate/"
  },
  "issues": {
    "url": "https://github.com/Quantiphi/webhook-boilerplate/issues"
  },
  "homepage": "https://github.com/Quantiphi/webhook-boilerplate#readme",
  "dependencies": {
    "dialogflow-fulfillment": "Quantiphi/dialogflow-fulfillment-builder",
    "dialogflow-fulfillment-builder": "github:Quantiphi/dialogflow-fulfillment-builder",
    "express": "^4.16.3",
    "express-winston": "^4.0.1",
    "helmet": "^3.21.2",
    "semver": "^7.3.2",
    "winston": "^3.0.0"
  },
  "keywords": [
    "webhook framework",
    "dialogflow webhook",
    "virtual agent",
    "boilerplate",
    "dialogflow boilerplate",
    "fulfillment service"
  ],
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "chai": "^4.3.6",
    "mocha": "^9.2.1",
    "typescript": "^4.6.2"
  }
}
